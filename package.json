  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "1.0.0-next.30",
    "@sveltejs/adapter-node": "1.0.0-next.47",
    "@sveltejs/adapter-static": "1.0.0-next.18",
    "@sveltejs/adapter-vercel": "1.0.0-next.28",
    "@sveltejs/kit": "next",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "flexsearch": "0.7.21",
    "front-matter": "4.0.2",
    "highlight.js": "11.2.0",
    "jsesc": "3.0.2",
    "marked": "3.0.4",
    "prettier": "2.4.0",
    "prettier-plugin-svelte": "2.4.0",
    "sass": "1.41.0",
    "svelte": "3.42.5",
    "svelte-preprocess": "4.9.4",
    "carbon-icons-svelte": "10.38.0",
    "daisyui": "1.14.0",
    "svelte-typewriter": "2.4.5",
    "vite-plugin-windicss": "1.4.3"
